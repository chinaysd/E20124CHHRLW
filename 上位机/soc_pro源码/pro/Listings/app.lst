1054          #if SYS_ENABLE_I2C
              void I2c_Irq() interrupt 7{
                     /*<主机发送数据>*/
                     if( (SSCON0&0x07) == 0x02){
                            unsigned char get_data = SSDAT;
                            if(get_data != I2C_ADDR){
                                   App_LedHandle(get_data);
                            }
                     }
                     /*<主机接收数据>*/
                     else if( (SSCON0&0x07) == 0x03 ){
                            if(I2C_IntGet() == 0){
                                   I2C_IntSet(1);
                            }
                     }
                     /*<必须手动清掉此位>*/      
                     SSCON0 &= ~0X40;
              }
              #endif
